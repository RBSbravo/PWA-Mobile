{
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "vite",
  "env": {
    "VITE_API_URL": "https://ticketing-and-task-management-system-production.up.railway.app/api",
    "VITE_SOCKET_URL": "https://ticketing-and-task-management-system-production.up.railway.app"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
