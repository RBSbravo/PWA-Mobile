{
  "name": "mito-pwa-mobile-app",
  "version": "1.0.0",
  "description": "MITO Task Management System - Progressive Web App",
  "main": "src/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:prod": "vite build",
    "serve": "vite preview --port 3000",
    "test:pwa": "npm run build:prod && npx lighthouse dist/index.html --view",
    "analyze": "npm run build:prod && npx webpack-bundle-analyzer dist/assets/*.js",
    "clean": "rm -rf dist node_modules && npm install",
    "clean:build": "rm -rf dist && npm run build:prod"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "@mui/material": "^5.11.0",
    "@mui/icons-material": "^5.11.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "socket.io-client": "^4.8.1",
    "workbox-window": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-background-sync": "^7.0.0",
    "workbox-broadcast-update": "^7.0.0",
    "idb": "^7.1.1",
    "react-pdf": "^7.0.0",
    "react-image-gallery": "^1.3.0",
    "react-dropzone": "^14.2.3",
    "date-fns": "^2.29.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
